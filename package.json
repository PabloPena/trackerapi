{
  "name": "trackerapi",
  "version": "1.0.0",
  "description": "URL Crawler",
  "nodemonConfig": {
    "ignore": [
      "**/*.test.ts",
      "**/*.spec.ts",
      ".git",
      "node_modules"
    ],
    "watch": [
      "src"
    ],
    "exec": "npm run serve",
    "ext": "ts"
  },
  "scripts": {
    "build": "tsc",
    "serve": "tsc && node --inspect=5858 -r ts-node/register ./dist/server.js",
    "start": "nodemon"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PabloPena/trackerapi.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/PabloPena/trackerapi/issues"
  },
  "homepage": "https://github.com/PabloPena/trackerapi#readme",
  "dependencies": {
    "debug": "^4.3.1",
    "ejs": "^3.1.5",
    "express": "^4.17.1",
    "morgan": "^1.10.0",
    "node-fetch": "^2.6.1",
    "swagger-ui-express": "^4.1.6",
    "uuid": "^8.3.2",
    "xmldom": "^0.4.0"
  },
  "devDependencies": {
    "@types/debug": "^4.1.5",
    "@types/express": "^4.17.9",
    "@types/morgan": "^1.9.2",
    "@types/node": "^14.14.16",
    "@types/node-fetch": "^2.5.7",
    "@types/swagger-ui-express": "^4.1.2",
    "@types/uuid": "^8.3.0",
    "@types/xmldom": "^0.1.30",
    "nodemon": "^2.0.6",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3"
  }
}
